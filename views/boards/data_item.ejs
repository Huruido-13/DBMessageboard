<% if (val != null){ %>
    <tr class="row">
        <th >
            <a class="text-dark" href="/boards/home/<%=val.User.name %>/
                <%=val.userId %>/0">
            <%= val.User.name %></a></th>
        <td ><%= val.message %></td>
        <% 
        var d = new Date(val.createdAt);
        var dstr = d.getFullYear() + '-' + (d.getMonth() + 1) + '-' +
        d.getDate() + ' ' + d.getHours() + ':' + d.getMinutes()+
        ':' + d.getSeconds(); %>
        <td class="col-3"><%= dstr %></td> 
    </tr>
    <% } %>
